generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Ordinateur {
  Tag String  @id @unique
  Modele String
  Fabriquant String
}
model Apprenant {
  Matricule String @unique
  Prenom String
  Nom String
  Postnom String
  Date_de_naissance DateTime
  Adresse String
  Email String @unique
  Telephone String
}
model Cohorte {
  Code String @unique
  Description String
}
